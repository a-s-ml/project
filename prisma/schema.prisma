generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextIndex", "fullTextSearch"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model chat {
  id        Int      @id @unique(map: "unique_id") @default(autoincrement())
  id_str    String   @unique(map: "unique_idstr") @db.VarChar(255)
  chat      BigInt   @unique(map: "unique_chat")
  type      String   @default("1") @db.VarChar(255)
  bot       Int?     @default(0)
  date      DateTime @default(now()) @db.Timestamp(0)
  dateUnix  Int
  ref       String?  @db.VarChar(255)
  img1      String?  @db.LongText
  img2      String?  @db.LongText
  img3      String?  @db.LongText
  city      Int?     @default(1)
  gender    Int?
  status    Int      @default(1)
  name      String?  @db.VarChar(255)
  about     String?  @db.VarChar(255)
  find      Int      @default(3)
  birthday  Int?
  target    Int?
  work      String?  @db.VarChar(255)
  education String?  @db.VarChar(255)
}

model complaints {
  id       Int      @id @default(autoincrement())
  type     Int
  to       BigInt
  from     BigInt
  date     DateTime @default(now()) @db.Timestamp(0)
  dateUnix Int?
}

model complaints_type {
  id     Int     @id @default(autoincrement())
  active Int?
  name   String? @db.VarChar(255)
}

model cities {
  id   Int     @id @default(autoincrement())
  name String? @db.VarChar(255)

  @@fulltext([name])
}

model find_type {
  id     Int     @id @default(autoincrement())
  active Int?
  name   String? @db.VarChar(255)
}

model genders_type {
  id     Int     @id @default(autoincrement())
  active Int?
  name   String? @db.VarChar(255)
}

model status_chat_type {
  id          Int     @id @unique(map: "id") @default(autoincrement())
  name        String? @db.VarChar(255)
  description String? @db.VarChar(255)
}

model reaction {
  id       Int       @id @unique(map: "id") @default(autoincrement())
  from     BigInt?
  to       BigInt?
  type     Int?
  date     DateTime? @default(now()) @db.Timestamp(0)
  dateUnix Int?
}

model interests {
  id   Int     @id @default(autoincrement())
  type Int
  name String? @db.VarChar(255)
  img  Int?    @default(128054)
}

model interests_type {
  id   Int     @id @unique(map: "id") @default(autoincrement())
  name String? @db.VarChar(255)
}

model privat_type {
  id   Int     @id @default(autoincrement())
  name String? @db.VarChar(255)
}

model target_type {
  id   Int     @id @default(autoincrement())
  name String? @db.VarChar(255)
}

model privatePr {
  id   Int     @id @default(autoincrement())
  type Int?
  name String? @db.VarChar(255)
}

model chat_interests {
  id          Int    @id @default(autoincrement())
  chat        BigInt
  interest_id Int
}

model chat_private {
  id    Int    @id @default(autoincrement())
  chat  BigInt
  type  Int
  value Int
}
